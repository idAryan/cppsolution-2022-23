#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int f_d,l_d,T,n;
    long long int num;
    scanf("%d",&T);
    n=0;
    while(n<T)
    {
        n++;
        scanf("%lld",&num);
        l_d=num%10;
        while(num>10)
        {
            num=num/10;
            f_d=num;
        }
    printf("%d\n",f_d+l_d);
        
    }
    

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
